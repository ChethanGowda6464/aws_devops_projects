pipeline 
{
    agent any
    environment
    {
     PATH = "/opt/apache-maven-3.6.3/bin:$PATH"
    }
    stages
    {
        stage("Git Clone") 
        {
            steps {
                git 'https://github.com/ChethanGowda6464/aws_devops_projects.git'
                  }
         }
            stage("Maven Builds")
            {
            steps {              
                sh "mvn validate"
                sh "mvn clean"
                sh "mvn compile"
                sh "mvn install"
                  }
            }
    }
}
